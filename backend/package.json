    "@zxcvbn-ts/language-en": "^3.0.2",
    "aws-sdk": "^2.1499.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.41.0",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "helmet": "^7.1.0",
