    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/testing": "^10.2.10",
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
